{
    "@context": "https://raw.githubusercontent.com/langens-jonathan/domain-json-ld-context/master/context.json",
    "version": "1.0.0",
    "resources": [
        {
            "name": "product",
            "iri": "ext:Product",
            "type": "resource",
            "properties": [
                {
                    "name": "title",
                    "iri": "dct:title",
                    "type": "string"
                },
                {
                    "name": "description",
                    "iri": "dct:description",
                    "type": "string"
                }
            ],
            "relationships": [
                {
                    "name": "tags",
                    "type": "hasMany",
                    "iri": "ext:tag",
                    "target": "tag"
                },
                {
                    "name": "manufacturer",
                    "type": "belongsTo",
                    "iri": "ext:manufacturer",
                    "target": "company"
                }
            ]
        },
        {
            "name": "tag",
            "iri": "ext:Tag",
            "type": "resource",
            "properties": [
                {
                    "name": "label",
                    "iri": "ext:label",
                    "type": "string"
                }
            ]
        },
        {
            "name": "company",
            "iri": "ext:Company",
            "type": "resource",
            "properties": [
                {
                    "name": "name",
                    "iri": "foaf:name",
                    "type": "string"
                },
                {
                    "name": "nrOfEmployees",
                    "iri": "ext:nrOfEmployees",
                    "type": "number"
                }
            ]
        }
    ]
}
