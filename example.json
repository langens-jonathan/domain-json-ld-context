{
    "@context": [
        "https://raw.githubusercontent.com/langens-jonathan/domain-json-ld-context/master/context.json",
        {
            "@base": "http://my-local-namespace.com/",
            "ext": "http://my-local-namespace.com/external/",
            "dct": "http://purl.org/dc/terms/",
            "foaf": "http://xmlns.com/foaf/0.1/"
        }
    ],
    "version": "1.0.0",
    "resources": [
        {
            "id": "Product",
            "name": "product",
            "type": "resource",
            "properties": [
                {
                    "name": "title",
                    "id": "dct:title",
                    "type": "string"
                },
                {
                    "name": "description",
                    "id": "dct:description",
                    "type": "string"
                }
            ],
            "relationships": [
                {
                    "name": "tags",
                    "type": "hasMany",
                    "id": "ext:tag",
                    "domain": {
                        "id": "Tag"
                    }
                },
                {
                    "name": "manufacturer",
                    "type": "belongsTo",
                    "id": "ext:manufacturer",
                    "domain": {
                        "id": "Company"
                    }
                }
            ]
        },
        {
            "id": "Tag",
            "name": "tag",
            "type": "resource",
            "properties": [
                {
                    "name": "label",
                    "id": "ext:label",
                    "type": "string"
                }
            ]
        },
        {
            "id": "Company",
            "name": "company",
            "type": "resource",
            "properties": [
                {
                    "name": "name",
                    "id": "foaf:name",
                    "type": "string"
                },
                {
                    "name": "nrOfEmployees",
                    "id": "ext:nrOfEmployees",
                    "type": "number"
                }
            ]
        }
    ]
}
